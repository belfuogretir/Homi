<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.belfu.homi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.onesignal.NotificationOpened.DEFAULT"
            android:value="DISABLE" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.NewPassActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.CreateGrupActivity" />
        <activity
            android:name=".Activity.ExpenseActivity"
            android:label="@string/title_activity_expense"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.GrupDetailActivity"></activity>
    </application>

</manifest>